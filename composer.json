{
    "name": "alecrabbit/php-color",
    "description": "Utilities for working with colors",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "color",
        "converter",
        "convert"
    ],
    "require": {
        "php": ">=8.2",
        "alecrabbit/php-color-models": "1.0.0-ALPHA.1"
    },
    "require-dev": {
        "nunomaduro/collision": "*",
        "phpunit/php-code-coverage": "*",
        "phpunit/phpunit": "^10.0",
        "symfony/console": "*",
        "symfony/var-dumper": "*"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "AlecRabbit\\Color\\": "src\\Color\\"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AlecRabbit\\Tests\\": "tests\\"
        }
    }
}
